{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "es2022", "webworker"],
    "strict": false,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "target": "es2022",
    "allowUmdGlobalAccess": true,
    "noImplicitAny": false,
    "types": []
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"],
  "compileOnSave": false
}